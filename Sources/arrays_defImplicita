using System; 
using static System.Console;

public class Program
{
  public static void Main()
  {
    var cifras = new[] {15_700.90, 21_450, 17_126.5, 12_900.34, 16_515,80};
    var años = new[] {2015, 2016, 2017, 2018, 2019};
    WriteLine("Arrays implícitamente inicializados");
    WriteLine($"Tipo cifras: {cifras.GetType()}");
    WriteLine($"Tipo cifras: {años.GetType()}");
    for(int i = 0; i < años.Length; i++)
      WriteLine($"{años[i]}) ${cifras[i]}");
  }
}
