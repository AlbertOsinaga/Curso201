using System;
using static System.Console;

public class Programa
{
  public static void Main()
  {
    InicializaValores(out int a, out string b, out bool c);
    WriteLine($"a = {a}, b = {b}, c = {c}");
  }

  static void InicializaValores(out int a, out string b, out bool c)
  {
    a = 9;
    b = "El mundo es nuevo...";
    c = true;
  }
}
