using System;
using static System.Console;

public class Programa
{
  public static void Main()
  {
    int n = 34; int m = 845;
    WriteLine($"n = {n}; m = {m}");
    Intercambia(n, m);
    WriteLine($"n = {n}; m = {m}");
  }

  public static void Intercambia(int x, int y)
  {
    int z = x;
    x = y;
    y = z;
  }
}
