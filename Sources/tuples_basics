using System;
using static System.Console;

public class Program
{
  public static void Main()
  {
    var tuple = (45, "Error", true); 
    WriteLine($"Tuple tuple = {tuple}");
    WriteLine($"tuple.Item1 : {tuple.Item1}");
    WriteLine($"tuple.Item2 : {tuple.Item2}");
    WriteLine($"tuple.Item3 : {tuple.Item3}");
    WriteLine($"Type Tuple : {tuple.GetType()}");
  }
}