using System;
using static System.Console;

public class Programa
{
  public static void Main()
  {
    WriteLine($"Suma ints = {Calculadora.Suma(145, 329)}");
    WriteLine($"Suma doubles = {Calculadora.Suma(145.90, 329.45)}");
    WriteLine($"Suma decimals = {Calculadora.Suma(195.87M, 336.78M)}");
  }
}
public static class Calculadora
{
  public static int Suma(int x, int y) => x + y;
  public static double Suma(double x, double y) => x + y;
  public static decimal Suma(decimal x, decimal y) => x + y;
} 