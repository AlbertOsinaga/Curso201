public static void Main()
  {
    Vector v; v.xÂ = 37.7; v.y = 26.5;
    Vector z = new Vector { x = 500.45, y = 455.23}; 
    WriteLine($"Vector v = {v}");
    WriteLine($"Vector z = {z}");
  }
  private struct Vector
  {
    public double x;
    public double y;
    
    public Vector(double t, double u)
    {
      x = t; y = u;
    }
    public override string ToString()
    {
      return $"({x}, {y})";
    }
  }
}
