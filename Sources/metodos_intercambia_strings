using System;
using static System.Console;

public class Programa
{
  public static void Main()
  {
    string m = "Ad√°n"; string f = "Eva";
    WriteLine($"m = {m}; f = {f}");
    Intercambia(m, f);
    WriteLine($"m = {m}; f = {f}");
  }

  public static void Intercambia(string x, string y)
  {
    string z = x;
    x = y;
    y = z;
  }
}
