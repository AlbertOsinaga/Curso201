using System;
using static System.Console;

public class Program
{
  public static void Main()
  {
    // Array de strings
    var mesesno31 = new [] {"Febrero", "Abril", "Junio", "Septiembre","Noviembre"};
    WriteLine("Meses del año que no tienen 31 días");
    foreach(string mes in mesesno31)
      WriteLine(mes);
  }
}