using System;
using static System.Console;

public class Programa
{
  public static void Main()
  {
    WriteLine($"SumaInt = {Calculadora.SumaInt(145, 329)}");
    WriteLine($"SumaDouble = {Calculadora.SumaDouble(145.90, 329.45)}");
    WriteLine($"SumaDecimal = {Calculadora.SumaDecimal(195.87M, 336.78M)}");
  }
}
public static class Calculadora
{
  public static int SumaInt(int x, int y) => x + y;
  public static double SumaDouble(double x, double y) => x + y;
  public static decimal SumaDecimal(decimal x, decimal y) => x + y;
} 